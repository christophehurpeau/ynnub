  "scripts": {
    "build": "npm run clean && NODE_ENV=production next build && NODE_ENV=production next export && touch out/.nojekyll",
    "clean": "rm -rf .next out",
    "lint": "tsc && eslint --ext .ts,.tsx src/",
    "serve": "serve out",
    "start": "next dev --port=3100",
    "start-prod": "next start"
